{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "outDir": "../../dist"
  },
  "filesGlob": [
    "./**/*.ts"
  ],
  "files": [
    "./server.ts",
    "./client.ts",
    "./auth/index.ts",
    "./auth/Credential.ts",
    "./auth/HalcyonToken.ts",
    "./mysql/index.ts",
    "./mysql/models/halcyon/users.ts",
    "./mysql/models/mgm/hosts.ts"
  ],
  "atom": {
    "rewriteTsconfig": true
  }
}
